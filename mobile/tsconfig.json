{
    "compilerOptions": {
        "strict": true,
        "paths": {
            "@/*": [
                "./*"
            ]
        },
        "target": "ES2020",
        "module": "commonjs",
        "lib": [
            "ES2020"
        ],
        "jsx": "react-native",
        "moduleResolution": "node",
        "allowSyntheticDefaultImports": true,
        "esModuleInterop": true,
        "skipLibCheck": true
    },
    "extends": "expo/tsconfig.base"
}